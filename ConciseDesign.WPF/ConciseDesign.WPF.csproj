<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>library</OutputType>
    <UseWPF>true</UseWPF>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <TargetFrameworks>net472;netcoreapp3.1</TargetFrameworks>
    <RootNamespace>ConciseDesign.WPF</RootNamespace>
    <AssemblyName>ConciseDesign.WPF</AssemblyName>
    <WarningLevel>4</WarningLevel>
    <UserSecretsId>af371226-ad84-4862-ad23-b30a8fdc5121</UserSecretsId>
    <Version>3.4.0</Version>
    <Description>WPF Light UI Library (under development)</Description>
