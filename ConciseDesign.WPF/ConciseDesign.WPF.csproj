    <PackageProjectUrl>https://github.com/migeyusu/ConciseDesign</PackageProjectUrl>
    <RepositoryUrl>https://github.com/migeyusu/ConciseDesign</RepositoryUrl>
    <RepositoryType>.Net</RepositoryType>
    <PackageTags>WPF</PackageTags>
    <PackageReleaseNotes>include materialdesign colors</PackageReleaseNotes>
    <PackageVersion>3.15.0</PackageVersion>
    <Title>ConciseDesign.WPF</Title>
    <Authors>Karl.Zhu</Authors>
    <AssemblyVersion>3.6.3</AssemblyVersion>
